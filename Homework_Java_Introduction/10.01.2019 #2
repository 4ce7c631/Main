import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        System.out.println("===============================================================================================================================================================");
        System.out.println("Made by 4ce7c631 | [WEB] https://github.com/4ce7c631");
        System.out.println("===============================================================================================================================================================");
        System.out.println("      ______  ________  ______  ________ ________________");
        System.out.println("     /  __  \\/   ____/ /  __  \\/    ___//   ____// ______\\");
        System.out.println("    /  / /  /  ___/   /  / /  /\\____  \\/  ___/  / /");
        System.out.println("   /  /_/  /  /_____ /  /_/  /_____/  /  /_____/ \\_______");
        System.out.println("  /_______/________//_______//_______/________/\\________/");
        System.out.println(" ");
        Scanner sc = new Scanner(System.in);
        System.out.println("Введите сторону a");
        double a = sc.nextInt();
        System.out.println("Введите сторону b");
        double b = sc.nextInt();
        System.out.println("Введите сторону c");
        double c = sc.nextInt();
        System.out.println("Введите массу груза");
        double m = sc.nextInt();
        System.out.println(isRight(a, b, c, m));
        }

    static String isRight(double a, double b, double c, double m) {
        String result;
        if (a <= 1 && b <= 1 && m <= 1) {
            return result = "Влезет влезет";
        }
        else if (b <= 1 && c <= 1 && m <= 1) {
            return result = "Влезет влезет";
        }
        else if (a <= 1 && c <= 1 && m <= 1) {
            return result = "Влезет влезет";
        }
        return result = "Все пропало";
    }
}
