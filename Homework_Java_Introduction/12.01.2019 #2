import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        System.out.println("===============================================================================================================================================================");
        System.out.println(" [WEB] https://github.com/4ce7c631");
        System.out.println("===============================================================================================================================================================");
        System.out.println("      ______  ________  ______  ________ ________________");
        System.out.println("     /  __  \\/   ____/ /  __  \\/    ___//   ____// ______\\");
        System.out.println("    /  / /  /  ___/   /  / /  /\\____  \\/  ___/  / /");
        System.out.println("   /  /_/  /  /_____ /  /_/  /_____/  /  /_____/ \\_______");
        System.out.println("  /_______/________//_______//_______/________/\\________/");
        System.out.println(" ");
        Scanner sc = new Scanner(System.in);
        int a;
        do {
            System.out.println("Введите сколько грамм ботнариума мы добыли (от 0 до 9) что бы мы зашифровали данные");
            a = sc.nextInt();
        }
        while (a < 0 | a > 9);
        long b = pow(a);
        System.out.println(b);
    }

    static long pow(int a) {
        double b = Math.ceil((Math.random() + a) * Math.pow(10, 11));
        return (long) b;
    }
}
